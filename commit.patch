+                var contents = $(this).contents();
+                
+                if(contents.length == 1 
+                    && contents[0].nodeType == this.TEXT_NODE
+                    && !/^[#@]+/.test(contents[0].textContent)
+                    && $this.attr('href').indexOf('//'+location.hostname+'/') < 0) {
+
