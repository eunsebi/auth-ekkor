+
+@Transactional
+class SanitizeService {
+
+    private static final AttributePolicy INTEGER = new AttributePolicy() {
+        @Override
+        String apply(String elementName, String attributeName, String value) {
+            int n = value.length()
+            if (n == 0) {
+                return null
+            }
+            for (int i = 0; i < n; ++i) {
+                char ch = value.charAt(i)
+                if (ch == '.') {
+                    if (i == 0) {
+                        return null
+                    }
+                    return value.substring(0, i)
+                } else if (!('0') <= ch && ch <= '9') {
+                    return null
+                }
+            }
+            return value
+        }
+    }
