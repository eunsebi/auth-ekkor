+					return;
+				}
+				
+				$.ajax({
+					url : autoCheckURL,
+					dataType : "json",
+					type : "post",
+					cache : false,
+					data : "cdtime="+ second_data.totime,
+					timeout: 3000,
+					success : function(data) {
