+								
+							} else if (data.type == "STOP") {
+								procAction("ERROR", DualAuthErr.getMsg("Z10.2"));
+								
+							} else if (data.type == "CONTINUE") {
+								makepollingObj()								
+							} else {
+								if (pollingObj != null) {
+									option.procAction("ERROR", DualAuthErr.getMsg("Z10.3"));
+								}
+							}
+						}	
+					},
+					error : function (e) {
+						callback.cancelCD($("#username").val());
+						callback.cancelSession();
